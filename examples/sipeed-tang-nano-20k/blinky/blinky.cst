IO_LOC "led"      15; // Active low
IO_PORT "led" PULL_MODE=UP DRIVE=8 BANK_VCCIO=1.8;

// Per https://github.com/YosysHQ/nextpnr/issues/1515
CLOCK_LOC "CLK" BUFG;

