//------------------------------------------------------------------
//-- Hello world example
//-- Turn on all the leds
//-- This example has been tested on the following boards:
//--   * Lattice icestick
//--   * Icezum alhambra (https://github.com/FPGAwars/icezum)
//------------------------------------------------------------------

module leds(output wire  [1:0] led,
			input wire [1:0] but
	);

assign led[0]=but[0];
assign led[1]=but[1];

endmodule
