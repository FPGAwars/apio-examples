# Capacitive-sense buttons
set_io -nowarn BTN1 136
set_io -nowarn BTN2 134
set_io -nowarn BTN3 128
set_io -nowarn BTN4 122

# LED outputs
set_io -nowarn LED1 137
set_io -nowarn LED2 135
set_io -nowarn LED3 129
set_io -nowarn LED4 121

# Clock input
set_io -nowarn CLK 49

# SPI Flash enable control
set_io -nowarn SS_B 71

# MCP3008 A/D
set_io -nowarn AD_CS   101
set_io -nowarn AD_Din  102
set_io -nowarn AD_Dout 104
set_io -nowarn AD_Clk  105

# USB
set_io -nowarn USB_Vp_o 112
set_io -nowarn USB_Vm_o 113
set_io -nowarn USB_Vm_i 114
set_io -nowarn USB_Vp_i 115
set_io -nowarn USB_nOE  116

set_io -nowarn ARDU00 138
set_io -nowarn ARDU01 141
set_io -nowarn ARDU02 143
set_io -nowarn ARDU03 1

# Arduino Power (P2)
# Active low RESET
#set_io -nowarn RESET_P2 73 -pullup yes
set_io -nowarn RESET_P2 73

# Milk channel A
set_io -nowarn Milk_TXD 37
set_io -nowarn Milk_RXD 38

